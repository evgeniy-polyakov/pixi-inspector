{
    "name": "pixi-inspector",
    "description": "Browsing and changing attributes of PixiJS display objects in DOM inspector.",
    "version": "1.0.7",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/evgeniy-polyakov/pixi-inspector.git"
    },
    "keywords": [
        "pixi",
        "pixijs",
        "inspector",
        "dom"
    ],
    "files": [
        "dist/",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "dependencies": {
        "pixi.js": "^5.3.9"
    },
    "devDependencies": {
        "lodash": "latest",
        "pixify": "latest",
        "rimraf": "latest",
        "typescript": "^4.2.4"
    },
    "scripts": {
        "build": "rimraf bin dist && tsc && pixify --name pixi-inspector -s bin -d dist --external false && tsc -p tsconfig-d.json"
    }
}
